package example.example;

import me.tobiasliese.jx_compiler.jvm.Test;

import example.example.Example2;
import example.example.Example3;

Example {
    render (String example, float test, Test test1) {
        <h1 id="hello" class="test">
            <h1>
                Hello world
            </h1>
            <br/>
            <h2>
                {example}
            </h2>
            {test}
            <h1>
                {test1.test(new ArrayList<>())}
            </h1>
            <Example2>
            </Example2>
            <Example3 hellUwU={"Hello world"} />
        </h1>
    }
    css {
        h1, h3 {
            background-color: black;
        }
        h2 {
            background-color: purple;
        }
        @media (hover: hover) {
            h1:hover {
                color: white;
            }
        }
    }
}
